        <mxCell id="f7_QPXtHo602dtLNfX0y-1" parent="1" style="whiteSpace=wrap;html=1;" value="Peer to peer system" vertex="1">
          <mxGeometry height="60" width="120" x="354" y="420" as="geometry" />
